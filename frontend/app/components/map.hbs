<LeafletMap
  id='map-cursor'
  @lat={{this.lat}}
  @lng={{this.lng}}
  @zoom={{this.zoom}}
  @onClick={{this.onMapClick}}
  @maxBounds={{this.bounds}}
  as |layers|
>
  {{! <layers.geojson @geoJSON='{this.geoData}' /> }}
  <layers.geojson
    @geoJSON={{this.geoData}}
    @style={{this.style}}
    @onEachFeature={{this.onEachFeature}}
  />
  <layers.tile
    @url='https://tile.informatievlaanderen.be/ws/raadpleegdiensten/wmts?SERVICE=WMTS&VERSION=1.0.0&REQUEST=GetTile&LAYER=grb_bsk_grijs&STYLE=&FORMAT=image/png&TILEMATRIXSET=GoogleMapsVL&TILEMATRIX={z}&TILEROW={y}&TILECOL={x}'
  />
</LeafletMap>
